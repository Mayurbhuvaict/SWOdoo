<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!-- Subscriber -->

        <service id="ICTECHOdooShopwareConnector\Subscriber\Admin\CountrySubscriber" public="true">
            <argument type="service" id="ICTECHOdooShopwareConnector\Components\Config\PluginConfig"/>
            <argument type="service" id="country.repository"/>
            <argument type="service" id="Psr\Log\LoggerInterface" />
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="ICTECHOdooShopwareConnector\Subscriber\Admin\PaymentMethodSubscriber" public="true">
            <argument type="service" id="ICTECHOdooShopwareConnector\Components\Config\PluginConfig"/>
